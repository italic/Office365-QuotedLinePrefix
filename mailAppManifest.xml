  </TabletSettings>
  <PhoneSettings>
    <!--To publish to the Add-in Store, enter the URL that this app will be hosted-->
    <SourceLocation DefaultValue="App/index.html" />
  </PhoneSettings>
  <Permissions>ReadItem</Permissions>
  <Rule xsi:type="RuleCollection" Mode="And"><!-- To add more complex rules, add additional rule elements --><Rule xsi:type="ItemIs" ItemType="Message" /><!-- E.g. To activate when a message contains an address --><!-- <Rule xsi:type="ItemHasKnownEntity" EntityType="Address" /> --></Rule>
